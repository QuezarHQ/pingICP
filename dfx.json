{
  "canisters": {
    "pingICP_backend": {
      "candid": "canisters/pingICP_backend/pingICP_backend.did",
      "package": "pingICP_backend",
      "type": "rust"
    },
    "assets": {
      "dependencies": [
        "pingICP_backend"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
